<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>
    <template class="ReglerWindow" parent="GtkApplicationWindow">
      <property name="default-width">300</property>
      <property name="default-height">300</property>
      <property name="resizable">False</property>
      <property name="window-position">center</property>
      <property name="gravity">center</property>

    <child type="titlebar">
      <object class="GtkHeaderBar" id="header_bar">
        <property name="visible">True</property>
        <property name="show-close-button">True</property>
        <property name="title">Regler Client</property>
      </object>
    </child>
    <child>
      <object class="GtkLayout">
        <property name="visible">True</property>
        <property name="can-focus">False</property>
        <child>
          <object class="GtkLabel">
            <property name="width-request">100</property>
            <property name="height-request">80</property>
            <property name="visible">True</property>
            <property name="can-focus">False</property>
            <property name="label" translatable="yes">Regler</property>
          </object>
          <packing>
            <property name="x">100</property>
            <property name="y">0</property>
          </packing>
        </child>
      <child>
          <object class="GtkButton">
            <property name="label" translatable="yes">Test</property>
            <property name="width-request">100</property>
            <property name="height-request">80</property>
            <property name="visible">True</property>
            <property name="can-focus">True</property>
            <property name="receives-default">True</property>
          </object>
          <packing>
            <property name="x">161</property>
            <property name="y">185</property>
          </packing>
      </child>
      </object>
    </child>
    </template>
  </interface>
